    "babel-eslint": "9.0.0",
    "connected-react-router": "4.5.0",
    "enzyme": "3.7.0",
    "inert": "^5.1.2",
    "jss": "latest",
    "keymirror": "^0.1.1",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "react": "16.6.3",
    "react-dom": "16.5.2",
    "react-jss": "latest",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
